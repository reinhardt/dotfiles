!XTerm.vt100.color22: rgb:30/88/88
!XTerm.vt100.color23: rgb:D8/D8/D8

XTerm*allowTitleOps: true
XTerm*allowSendEvents: false

XTerm.termName: xterm-256color
XTerm.vt100.locale: true
XTerm.vt100.metaSendsEscape: true
XTerm.vt100.translations: #override \n\
    Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
    Ctrl Shift <KeyPress> KP_Add: larger-vt-font() \n\
    Ctrl Shift <KeyPress>=: smaller-vt-font()
!XTerm.*backarrowKey: false
!XTerm*ttyModes:         erase ^?
!XTerm*VT100.translations: \
!        #override <Key>BackSpace: string(0x7f) \n\
!		          <Key>Delete:    string(0x1b) string("[3~")
XTerm.vt100.saveLines: 16384
XTerm.vt100.scrollBar: false

URxvt.perl-ext-common: default,matcher,keyboard-select,vtwheel
URxvt.keyboard-select.clipboard: true
URxvt.clipboard.autocopy: true
Rxvt.saveLines: 16384

! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
! disable the scrollback buffer for the secondary screen
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

! Disable ISO 14755 unicode input so we can use Ctrl-Shift bindings
URxvt.iso14755:        false
URxvt.iso14755_52:     false

! Bind Ctrl-Shift-c & Ctrl-Shift-v to copy and paste
! I dont know why, but I needed to use hex keysym values to get it to work
URxvt.keysym.Shift-Control-v: eval:paste_clipboard
URxvt.keysym.Shift-Control-c: eval:selection_to_clipboard
!URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
!URxvt.keysym.C-S-0x56: eval:paste_clipboard
URxvt.keysym.M-s: searchable-scrollback:start
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-C-1: command:\033]710;xft:Monospace:size=7\007
URxvt.keysym.M-C-2: command:\033]710;xft:Monospace:size=9\007
URxvt.keysym.M-C-3: command:\033]710;xft:Monospace:size=11\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

URxvt.url-launcher: /run/current-system/sw/bin/xdg-open
!URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-u: matcher:select
URxvt.keysym.M-i: matcher:list
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5

URxvt.shading: 8
URxvt.transparent: false
