Xft.dpi: 192
Xcursor.theme: Obsidian
Xcursor.size: 64

XTerm*pointerSize: 128
XTerm*pointerColor: rgb:ec/52/af
XTerm*pointerColorBackground: rgb:65/7B/83
XTerm*pointerShape: text
!XTerm.vt100.faceName: Monospace:size=7.5
XTerm.vt100.faceName: Monospace:size=9
XTerm*cursorBlink: true
XTerm*cursorOffTime: 300
XTerm*cursorOnTime: 1800
*foreground: rgb:65/7B/83
*background: rgb:FD/F6/E3
*color0: rgb:ee/e8/d5
*color1: rgb:dc/32/2f
